{
    "name": "prductloader",
	"build": {
		"dockerfile": "Dockerfile",
		"context": "../"
	},
	"customizations": {
		"vscode": {
			"settings": {
				"terminal.integrated.shell.linux": "/bin/bash",
				"python.pythonPath": "/usr/local/bin/python"
			},
			"extensions": [
				"ms-python.python"
			]
		}
	},
	"runArgs": [
		"--network=ecommerce-network",
		"--env-file", "${localWorkspaceFolder}/.devcontainer/.env"
	]
}